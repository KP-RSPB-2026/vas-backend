# Server Configuration
PORT=3000
NODE_ENV=development
APP_URL=http://localhost:3000

# MySQL Configuration
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_USER=root
MYSQL_PASSWORD=yourpassword
MYSQL_DB=vas_app
MYSQL_CONN_LIMIT=10

# JWT Secrets
JWT_SECRET=your_jwt_secret_here
JWT_REFRESH_SECRET=your_jwt_refresh_secret_here
ACCESS_TOKEN_EXPIRES_IN=3600        # seconds (default 1h)
REFRESH_TOKEN_EXPIRES_IN=604800     # seconds (default 7d)

# Working Hours
WORK_START_HOUR=7
WORK_START_MINUTE=0
WORK_END_HOUR=16
WORK_END_MINUTE=0
